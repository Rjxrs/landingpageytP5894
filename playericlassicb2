            <!-- Right HUD Panel -->
            <div class="w-80 bg-zinc-950 border-l border-zinc-800 p-6 flex flex-col">
                <div>
                    <div class="flex items-center gap-2 mb-4">
                        <div class="w-2 h-2 bg-emerald-500 rounded-full animate-pulse"></div>
                        <span class="uppercase text-xs font-medium tracking-widest text-emerald-400">SECURE TUNNEL</span>
                    </div>
                    <div id="status" class="font-mono text-xs bg-black/50 border border-zinc-700 rounded-xl px-4 py-3 text-zinc-400">
                        IDLE • AWAITING BYPASS COMMAND
