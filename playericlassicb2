                <div>
                    <h1 class="text-3xl font-black tracking-[-2px]">GRAAL CLASSIC</h1>
                    <p class="text-[10px] text-emerald-400 tracking-[3px] -mt-1">SECURE PLAYER-ONLY WRAPPER</p>
                </div>
            </div>
            
            <div class="flex items-center gap-x-6">
                <!-- Proxy Engine -->
                <div class="flex items-center gap-x-3 bg-zinc-900 border border-zinc-700 rounded-2xl px-5 py-2.5">
                    <span class="text-xs uppercase tracking-widest text-zinc-400">ENGINE</span>
                    <select id="proxy-select" class="bg-transparent text-emerald-400 font-medium focus:outline-none text-sm">
                        <option value="alpha">ALPHA — AllOrigins</option>
                        <option value="beta">BETA — CorsProxy.io</option>
                        <option value="gamma">GAMMA — CodeTabs</option>
                    </select>
                </div>

                <!-- Session ID -->
                <div class="flex items-center gap-x-3 bg-zinc-900 border border-zinc-700 rounded-2xl px-5 py-2.5 font-mono">
                    <span class="text-emerald-400"><i class="fas fa-fingerprint"></i></span>
                    <span id="session-display" class="text-emerald-300 text-sm">SID-00000000</span>
                    <button onclick="copySession()" class="text-zinc-400 hover:text-white transition-colors">
                        <i class="fas fa-copy text-xs"></i>
                    </button>
                </div>

                <!-- Execute -->
                <button onclick="executeBypass()" 
                        id="execute-btn"
                        class="flex items-center gap-x-3 bg-emerald-500 hover:bg-emerald-600 active:scale-95 transition-all text-black font-semibold px-8 py-3.5 rounded-2xl shadow-lg shadow-emerald-500/50">
                    <i class="fas fa-bolt"></i>
                    <span class="uppercase tracking-widest text-sm">EXECUTE BYPASS</span>
                </button>

                <!-- Fullscreen -->
                <button onclick="toggleFullscreen()" 
                        class="p-3.5 bg-zinc-900 hover:bg-zinc-800 rounded-2xl transition-colors">
                    <i class="fas fa-expand text-xl"></i>
                </button>
            </div>
        </nav>

        <div class="flex flex-1 overflow-hidden">
            <!-- Viewport Container -->
            <div class="flex-1 flex items-center justify-center p-8 bg-zinc-900">
                <div id="viewport" 
                     class="viewport relative w-[800px] h-[600px] bg-black border-[14px] border-emerald-500 rounded-3xl overflow-hidden ring-1 ring-inset ring-black">
                    
                    <!-- Iframe (1024px wide, cropped) -->
                    <iframe id="gameframe" 
                            class="absolute left-1/2 top-[-146px] -translate-x-1/2 w-[1024px] h-[746px]"
                            allow="fullscreen; accelerometer; gyroscope; magnetometer; autoplay; encrypted-media">
                    </iframe>

                    <!-- Subtle scanline overlay for retro feel -->
                    <div class="absolute inset-0 pointer-events-none bg-[repeating-linear-gradient(transparent_0px,transparent_2px,rgba(0,0,0,0.08)_2px,rgba(0,0,0,0.08)_4px)]"></div>
                </div>
