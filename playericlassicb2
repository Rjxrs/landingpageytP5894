            <div class="mt-10">
                <div class="text-emerald-400 text-2xl font-medium tracking-[6px]">RESOLVING CLOUDFLARE RAY</div>
                <div class="text-zinc-400 font-mono text-sm mt-3">X-FRAME-OPTIONS • CSP • BOT CHALLENGE</div>
            </div>
            
            <div id="overlay-status" class="mt-16 font-mono text-xs text-emerald-500/80 tracking-widest">
                ESTABLISHING SECURE PROXY TUNNEL...
            </div>
        </div>
    </div>

    <script>
        // Tailwind already initialized via CDN
        const proxies = {
            alpha: { name: "AllOrigins", base: "https://api.allorigins.win/raw?url=" },
            beta: { name: "CorsProxy.io", base: "https://corsproxy.io/?" },
            gamma: { name: "CodeTabs", base: "https://api.codetabs.com/v1/proxy?quest=" }
        };

        let currentSessionId = "";
        let currentProxyKey = "alpha";

        function generateSessionId() {
            return "GRAAL-" + Math.random().toString(36).substring(2, 11).toUpperCase() + Date.now().toString(36).slice(-4).toUpperCase();
        }

        function log(msg, type = "info") {
            const term = document.getElementById("terminal");
            const time = new Date().toLocaleTimeString("en-US", { hour12: false, hour: "2-digit", minute: "2-digit", second: "2-digit" });
            let prefix = "[INFO]";
            let cls = "text-emerald-300";
            if (type === "success") { prefix = "[OK]"; cls = "text-emerald-400"; }
            else if (type === "error") { prefix = "[ERR]"; cls = "text-red-400"; }
            else if (type === "warn") { prefix = "[WARN]"; cls = "text-amber-400"; }
            
            const entry = document.createElement("div");
            entry.className = `${cls} mb-1`;
            entry.innerHTML = `<span class="text-zinc-500">${time}</span> ${prefix} ${msg}`;
            term.appendChild(entry);
            term.scrollTop = term.scrollHeight;
        }

        function updateSessionDisplay() {
            document.getElementById("session-display").textContent = currentSessionId;
        }

        function copySession() {
            if (!currentSessionId) return;
            navigator.clipboard.writeText(currentSessionId);
            const el = document.getElementById("session-display");
            const orig = el.textContent;
            el.textContent = "✓ COPIED";
            setTimeout(() => { el.textContent = orig; }, 1500);
        }

        function showOverlay() {
            document.getElementById("overlay").classList.remove("hidden");
        }

        function hideOverlay() {
            document.getElementById("overlay").classList.add("hidden");
        }

        function executeBypass() {
            const btn = document.getElementById("execute-btn");
            btn.disabled = true;
            btn.innerHTML = `<i class="fas fa-spinner fa-spin mr-2"></i>BOOTING...`;

            currentSessionId = generateSessionId();
            updateSessionDisplay();

            currentProxyKey = document.getElementById("proxy-select").value;
            const proxy = proxies[currentProxyKey];

            const timestamp = Math.floor(Date.now() / 1000);
            const targetUrl = `https://classic.graalonline.com/?sid=${encodeURIComponent(currentSessionId)}&ts=${timestamp}`;

            let proxyUrl = proxy.base + encodeURIComponent(targetUrl);
            if (currentProxyKey === "beta") {
                proxyUrl = proxy.base + encodeURIComponent(targetUrl); // already correct
            }

            log(`Bypass sequence started • ${proxy.name}`, "warn");
            log(`Session ID: ${currentSessionId} • Timestamp: ${timestamp}`, "info");
            log(`Proxying → ${targetUrl}`, "info");

            showOverlay();
            document.getElementById("overlay-status").textContent = `TUNNELING VIA ${proxy.name.toUpperCase()}...`;

            const iframe = document.getElementById("gameframe");
            iframe.src = proxyUrl;

            iframe.onload = () => {
                log("Proxy delivered page. Resolving internal challenges...", "success");
                setTimeout(() => {
                    hideOverlay();
                    log("Cloudflare Ray resolved • Player-only viewport ready", "success");
                    document.getElementById("status").innerHTML = `LIVE • ${proxy.name.toUpperCase()} TUNNEL`;
                    document.getElementById("status").classList.add("text-emerald-400");

                    btn.disabled = false;
                    btn.innerHTML = `<i class="fas fa-bolt"></i><span class="uppercase tracking-widest text-sm ml-2">RE-EXECUTE BYPASS</span>`;
                }, 3000);
            };

            iframe.onerror = () => {
                hideOverlay();
                log("Proxy connection failed. Try alternate engine.", "error");
                btn.disabled = false;
                btn.innerHTML = `<i class="fas fa-bolt"></i><span class="uppercase tracking-widest text-sm ml-2">EXECUTE BYPASS</span>`;
            };
        }

        function toggleFullscreen() {
            const vp = document.getElementById("viewport");
            if (!document.fullscreenElement) {
                vp.requestFullscreen().catch(() => {});
            } else {
                document.exitFullscreen();
            }
        }

        function clearLog() {
            document.getElementById("terminal").innerHTML = "";
            log("Terminal cleared", "info");
        }

        // Live clock
        function startClock() {
            setInterval(() => {
                const now = new Date();
                document.getElementById("live-clock").textContent = now.toISOString().slice(0, 19).replace("T", " ") + " UTC";
            }, 1000);
        }

        // Keyboard support
        document.addEventListener("keydown", (e) => {
            if (e.key === "F11" || (e.key === "f" && e.ctrlKey)) {
                e.preventDefault();
                toggleFullscreen();
            }
            if (e.key === "Enter" && !document.getElementById("execute-btn").disabled) {
                executeBypass();
            }
        });

        // Init
        window.onload = () => {
            startClock();
            currentSessionId = generateSessionId();
            updateSessionDisplay();
            
            log("GRAAL CLASSIC PROXY WRAPPER v1.0 INITIALIZED", "success");
            log("Security bypass ready • Select engine & EXECUTE", "info");
            
            // Pre-select Beta (most reliable for full pages)
            document.getElementById("proxy-select").value = "beta";
        };
    </script>
</body>
</html>
