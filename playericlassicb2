    <!-- Cloudflare-style Loading Overlay -->
    <div id="overlay" class="hidden fixed inset-0 bg-zinc-950/95 backdrop-blur-3xl z-[9999] flex items-center justify-center">
        <div class="text-center">
            <div class="flex justify-center">
                <div class="relative">
                    <div class="w-20 h-20 border-8 border-zinc-700 rounded-full"></div>
